function CheckAnyPlayerDistnaceInCFrame(u, x)
    for r, v in pairs(game.Players:GetChildren()) do
        pcall(
            function()
                if
                    v.Name ~= game.Players.LocalPlayer.Name and
                        (v.Character.HumanoidRootPart.Position - u.Position).Magnitude <= x
                 then
                    return v
                end
            end
        )
    end
end
