function Notify(G, H, I)
    if G == nil or G == "" then
        G = "Not Titled"
    end
    if H == nil or H == "" then
        H = "No Any Descriptions"
    end
    if type(I) ~= "number" then
        I = 10
    end
    z:MakeNotification({Name = G, Content = H, Image = MoonTextureId(), Time = I})
end
