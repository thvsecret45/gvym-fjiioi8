                                (game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position -
                                    CFrame.new(
                                        -1404.07996,
                                        29.8520069,
                                        5.26677656,
                                        0.888123989,
                                        -4.0340602e-09,
                                        0.459603906,
                                        7.5884703e-09,
                                        1,
                                        -5.8864642e-09,
                                        -0.459603906,
                                        8.71560069e-09,
                                        0.888123989
                                    )).Magnitude > 8
                             then
                                toTarget(
                                    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,
                                    CFrame.new(
                                        -1404.07996,
                                        29.8520069,
                                        5.26677656,
                                        0.888123989,
                                        -4.0340602e-09,
                                        0.459603906,
                                        7.5884703e-09,
